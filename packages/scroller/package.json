{
  "name": "@smoovy/scroller",
  "version": "0.0.16",
  "description": "Smooth scrolling for the world",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "types": "dist/typings/index.d.ts",
  "homepage": "https://github.com/davideperozzi/smoovy",
  "repository": "https://github.com/davideperozzi/smoovy",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**",
    "m/**"
  ],
  "scripts": {
