{
  "name": "@smoovy/component",
  "version": "0.2.0",
  "description": "Simple component manager",
  "main": "dist/bundles/index.js",
  "module": "dist/bundles/index.esm.js",
  "unpkg": "dist/bundles/index.umd.js",
  "source": "src/index.ts",
  "types": "dist/typings/index.d.ts",
  "homepage": "https://github.com/davideperozzi/smoovy",
  "repository": "https://github.com/davideperozzi/smoovy",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist/**"
  ],
  "scripts": {
    "lint": "NAME=$(basename \"$PWD\") && cd ../../ && npm run lint:package $NAME",
    "test": "NAME=$(basename \"$PWD\") && cd ../../ && npm run test:package $NAME",
    "build": "NAME=$(basename \"$PWD\") && cd ../../ && npm run build:package $NAME",
    "serve": "NAME=$(basename \"$PWD\") && cd ../../ && npm run serve:package $NAME $1"
  },
  "keywords": [
    "smoovy",
    "component",
    "manager"
  ],
  "author": "Davide Perozzi",
  "license": "MIT",
  "dependencies": {
    "@smoovy/event": "^0.2.1",
    "@smoovy/observer": "^0.5.0",
    "@smoovy/utils": "^0.1.16",
    "tslib": "^1.9.3"
  }
}
